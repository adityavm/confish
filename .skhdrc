cmd + ctrl - h : yabai -m window --focus north
cmd + ctrl - l : yabai -m window --focus south
cmd + ctrl - k : yabai -m window --focus east
cmd + ctrl - j : yabai -m window --focus west
cmd + ctrl - a : yabai -m space --focus a
cmd + ctrl - d : yabai -m space --focus d
cmd + ctrl - c : yabai -m space --focus c
cmd + ctrl - w : yabai -m space --focus w
cmd + ctrl - 5 : yabai -m space --focus 5
cmd + ctrl - 6 : yabai -m space --focus 6
cmd + ctrl - 7 : yabai -m space --focus 7
cmd + ctrl - 8 : yabai -m space --focus 8
cmd + ctrl - 9 : yabai -m space --focus 9

cmd + ctrl - left : yabai -m window --resize left:-20:0
cmd + ctrl + alt - left : yabai -m window --resize left:20:0
cmd + ctrl - right : yabai -m window --resize right:20:0
cmd + ctrl + alt - right : yabai -m window --resize right:-20:0
cmd + ctrl - up : yabai -m window --resize top:0:-20
cmd + ctrl + alt - up : yabai -m window --resize top:0:20
cmd + ctrl - down : yabai -m window --resize bottom:0:20
cmd + ctrl + alt - down : yabai -m window --resize bottom:0:-20

cmd + ctrl - m : yabai -m window --space next
cmd + ctrl - n : yabai -m window --space prev
cmd + ctrl + alt - n : yabai -m space --create
cmd + ctrl + alt - m : yabai -m space --destroy

cmd + ctrl - return : yabai -m window --toggle float